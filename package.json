{
  "name": "stream-string",
  "description": "Converts a stream to a string and supports promises.",
  "version": "1.0.2",
  "author": "James Costian <james@jamescostian.com>",
  "bugs": {
    "url": "https://github.com/jamescostian/stream-string/issues"
  },
  "devDependencies": {
    "coveralls": "^2",
    "nyc": "^10",
    "snazzy": "^5",
    "tap-spec": "^4",
    "tape": "^4"
  },
  "homepage": "https://github.com/jamescostian/stream-string",
  "keywords": [
    "stream"
  ],
  "license": "ISC",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamescostian/stream-string.git"
  },
  "scripts": {
    "coveralls": "nyc --reporter=text-lcov node test/test.js | coveralls",
    "cov": "nyc --reporter=html --reporter=text node test/test.js",
    "pretest": "snazzy",
    "test": "node test/test.js | tap-spec"
  },
  "nyc": {
    "lines": 99,
    "statements": 99,
    "functions": 99,
    "branches": 99,
    "include": [
      "index.js"
    ],
    "cache": true,
    "all": true
  }
}
